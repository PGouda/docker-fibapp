{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions":[
    {
      "name": "client",
      "image": "sharangouda/docker-fib-client",
      "hostname": "client",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": "3000",
          "hostPort": "3000"
        }
      ]
    },
    {
      "name": "server",
      "image": "sharangouda/docker-fib-server",
      "hostname": "react-app",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": "5000",
          "containerPort": "5000"
        }
      ]
    },
    {
      "name": "worker",
      "image": "sharangouda/docker-fib-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "ngnix",
      "image": "sharangouda/docker-fib-ngnix",
      "hostname": "ngnix",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": "80",
          "containerPort": "80"
        }
      ],
      "links": ["client","server"]
    }
  ]
}
